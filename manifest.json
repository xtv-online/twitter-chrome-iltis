{
    "author": "Sam Hutchings",
    "content_scripts": [
        {
            "matches": ["*://twitter.com/*"],
            "js": [
                "libs/jquery.min.js",
                "index.js"
            ]
        }
    ],
    "default_locale": "en",
    "description": "__MSG_description__",
    "homepage_url": "https://github.com/xtv-online",
    "icons": {
        "16": "graphics/icon16.png",
        "48": "graphics/icon48.png",
        "128": "graphics/icon128.png"
    },
    "manifest_version": 2,
    "name": "XTV Twitter - iltis Tweet Sender",
    "version": "0.0.1",
    "background": {
        "scripts": ["libs/jquery.min.js", "background.js"]
    },
    "permissions": [
        "<all_urls>","webRequest","webRequestBlocking"
    ]
}
